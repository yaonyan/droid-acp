{
  "name": "@yaonyan/droid-acp",
  "type": "module",
  "version": "0.0.3",
  "description": "ACP Agent adapter for Factory Droid CLI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yaonyan/droid-acp.git"
  },
  "author": "",
  "license": "MIT",
  "bin": {
    "droid-acp": "./dist/index.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "start": "node dist/index.mjs",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.8.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "bumpp": "^10.3.2",
    "dotenv": "^17.2.3",
    "tsdown": "^0.17.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}